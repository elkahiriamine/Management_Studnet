<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="web.model.Etudiant" table="etudiant">
		<id name="id" column="idEtudiant">
			<generator class="native"/>
		</id>
		<property name="nom" column="nomEtudiant" />
		<property name="prenom" column="prenomEtudiant" />	
		<property name="age" column="ageEtudiant" />
	</class>

</hibernate-mapping>

